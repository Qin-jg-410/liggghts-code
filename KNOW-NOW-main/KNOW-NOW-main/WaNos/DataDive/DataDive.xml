<WaNoTemplate>

   <WaNoMeta>
      <Author>
         <Name> Celso R. C. Rego </Name>
         <Email>celsorego@kit.edu</Email>
      </Author>

      <Description>
         This WaNo uses Liggghts functionalities to simulate the stress-strain curve of a granular material.
      </Description>

      <Keyword>Liggghts</Keyword>
  </WaNoMeta>

   <WaNoRoot name="KNOW-NOW_eval">

      <WaNoFile logical_filename="forces_inputs.yml"  name="forces"> file path </WaNoFile>
      
      <WaNoFloat name="contact pressure">0.5</WaNoFloat>

      
 
     

   </WaNoRoot>
   <WaNoExecCommand>
      bash config.sh
   </WaNoExecCommand>
   
   <WaNoInputFiles> 
      <WaNoInputFile logical_filename="config.sh">config.sh</WaNoInputFile>
      <WaNoInputFile logical_filename="dem_stress_strain.py">dem_stress_strain.py</WaNoInputFile>      
   </WaNoInputFiles>
   
   <WaNoOutputFiles>
      <WaNoOutputFile>datadive_results.yml</WaNoOutputFile>
   </WaNoOutputFiles>
</WaNoTemplate>
